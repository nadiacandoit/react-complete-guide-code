sses.bump : ''}`;\r\n    useEffect(() => {\r\n        if (nullCondition) {\r\n            console.log(\"no item is in the cart\")\r\n            return;\r\n        }\r\n        setBtnIsHighlighted(true);\r\n\r\n        const timer = setTimeout(() => {\r\n            setBtnIsHighlighted(false);\r\n        }, 300);\r\n\r\n        //clean up \r\n        return () => {\r\n            clearTimeout(timer);\r\n        };\r\n    }, [items]);\r\n\r\n    return (\r\n        <Fragment>\r\n            <button className={btnClasses} onClick={props.onClick}>\r\n                <span className={classes.icon}> <CartIcon /></span>\r\n                <span>{btnName}</span>\r\n                <span className={classes.badge}>{numberOfCartItems}</span>\r\n            </button>\r\n        </Fragment>\r\n    );\r\n};\r\n\r\nexport default HeaderCartButton;\r\n","C:\\Users\\jiyou_folaqk\\Documents\\ReactProjects\\ch11\\code\\01-starting-project\\src\\components\\Cart\\CartIcon.js",[],"C:\\Users\\jiyou_folaqk\\Documents\\ReactProjects\\ch11\\code\\01-starting-project\\src\\components\\Meals\\MealsSummary.js",[],"C:\\Users\\jiyou_folaqk\\Documents\\ReactProjects\\ch11\\code\\01-starting-project\\src\\components\\Meals\\AvailableMeals.js",[],"C:\\Users\\jiyou_folaqk\\Documents\\ReactProjects\\ch11\\code\\01-starting-project\\src\\components\\Meals\\dummy-meals.js",["84","85"],"C:\\Users\\jiyou_folaqk\\Documents\\ReactProjects\\ch11\\code\\01-starting-project\\src\\components\\Meals\\Meals.js",[],"C:\\Users\\jiyou_folaqk\\Documents\\ReactProjects\\ch11\\code\\01-starting-project\\src\\components\\Meals\\MealItem\\MealItem.js",[],"C:\\Users\\jiyou_folaqk\\Documents\\ReactProjects\\ch11\\code\\01-starting-project\\src\\components\\Meals\\MealItem\\MealItemForm.js",[],"C:\\Users\\jiyou_folaqk\\Documents\\ReactProjects\\ch11\\code\\01-starting-project\\src\\components\\UI\\Input.js",[],"C:\\Users\\jiyou_folaqk\\Documents\\ReactProjects\\ch11\\code\\01-starting-project\\src\\components\\Cart\\Cart.